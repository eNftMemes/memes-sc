{
  "name": "voting add meme",
  "steps": [
    {
      "step": "externalSteps",
      "path": "voting-add_meme_alter_period.scen.json"
    },
    {
      "step": "scQuery",
      "txId": "currentPeriodLength",
      "tx": {
        "to": "sc:voting_contract",
        "function": "current_period_len",
        "arguments": []
      },
      "expect": {
        "out": [
          "2"
        ],
        "status": "0"
      }
    },
    {
      "step": "scQuery",
      "txId": "currentPeriodMemesLatest",
      "tx": {
        "to": "sc:voting_contract",
        "function": "current_period_memes_latest",
        "arguments": ["0"]
      },
      "expect": {
        "out": [
          "3",
          "2"
        ],
        "status": "0"
      }
    },
    {
      "step": "scQuery",
      "txId": "currentPeriodMemesLatest",
      "tx": {
        "to": "sc:voting_contract",
        "function": "current_period_memes_latest",
        "arguments": ["1"]
      },
      "expect": {
        "out": [],
        "status": "0"
      }
    },
    {
      "step": "scQuery",
      "txId": "periodMemesLatest",
      "tx": {
        "to": "sc:voting_contract",
        "function": "period_memes_latest",
        "arguments": ["123,000", "0"]
      },
      "expect": {
        "out": [
          "1"
        ],
        "status": "0"
      }
    },
    {
      "step": "scQuery",
      "txId": "periodMemes",
      "tx": {
        "to": "sc:voting_contract",
        "function": "period_memes",
        "arguments": ["727,800"]
      },
      "expect": {
        "out": [
          "2",
          "3"
        ],
        "status": "0"
      }
    },
    {
      "step": "scQuery",
      "txId": "periodMemes",
      "tx": {
        "to": "sc:voting_contract",
        "function": "period_memes",
        "arguments": ["123,000"]
      },
      "expect": {
        "out": [
          "1"
        ],
        "status": "0"
      }
    }
  ]
}